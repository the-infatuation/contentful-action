{
  "name": "contentful-migrations-action",
  "version": "3.2.0",
  "description": "JavaScript Action for Contentful migrations",
  "main": "src/index.ts",
  "scripts": {
    "format": "npx prettier --write \"**/*.{js,tsx,scss,ts}\"",
    "format:check": "npx prettier --check \"**/*.{js,tsx,scss,ts}\"",
    "lint": "npx xo src/*",
    "lint:fix": "npx xo src/* --fix",
    "package": "npm run ts:compile && npx ncc build build/index.js -o dist",
    "ts:compile": "npx tsc",
    "test": "npx xo src/* && jest"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Contentful"
  ],
  "author": "GitHub",
  "license": "MIT",
  "engines": {
    "npm": "^10.1.0",
    "node": "^20.8.0"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.5"
  },
  "dependencies": {
    "@actions/core": "^1.1.1",
    "@actions/github": "^6.0.0",
    "chalk": "^5.3.0",
    "contentful-management": "^10.6.2",
    "contentful-migration": "^4.9.1",
    "to-semver": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.19.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.38.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "xo": "^0.56.0"
  }
}
